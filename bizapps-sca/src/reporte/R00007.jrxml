	<queryString>
		<![CDATA[SELECT nombre, ficha, id_turno,cuadrilla,id_permiso FROM (SELECT     nombre,fecha_turno as fecha, ficha, id_turno,cuadrilla,id_permiso
FROM         planificacion_semanal
WHERE     fecha_turno = $P{fecha_desde} and id_turno in ($P!{turno})  AND    ficha not IN    (SELECT     distinct(ficha)   FROM   registro_acceso
                            WHERE   fecha = $P{fecha_desde}  )
UNION
SELECT     nombre,fecha_turno as fecha, ficha, id_turno,cuadrilla,id_permiso
FROM         planificacion_semanal where fecha_turno =$P{fecha_desde} and id_turno in ($P!{turno}) and ficha in (SELECT     distinct(ficha)   FROM   registro_acceso  WHERE   fecha =$P{fecha_desde} and ficha  in (select distinct(ficha) from planificacion_semanal where id_permiso<>'' and id_turno in ($P!{turno}) and fecha_turno =$P{fecha_desde}) )) as tmp
ORDER BY id_turno,cuadrilla]]>
